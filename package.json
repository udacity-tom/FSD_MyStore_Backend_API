{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "test": "jasmine-ts",
    "tsc": "tsc",
    "create-test":"npm run create-testdb-up && db-migrate --env test up && ENV=test jasmine-ts && npm run create-testdb-down",
    "create-testdb-up":"db-migrate up:setup && db-migrate --env test up && db-migrate --env test up:test",
    "create-testdb-down":"db-migrate --env test down:test && db-migrate --env test down && db-migrate down:setup"
    
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.17.1",
    "body-parser": "^1.19.0",
    "typescript": "^4.1.3",
    "jsonwebtoken": "^8.5.1",
    "pgtools": "^0.3.2",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/pg": "^7.14.7",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/jasmine": "^3.6.3",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@types/jsonwebtoken": "^8.5.5",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^1.19.1"
  }
}
